{
  "name": "spotify_with_friends",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit",
    "test-pa11y": "./node_modules/.bin/pa11y-ci",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false,screenshotOnRunFailure=false",
    "ci-pa11y": "start-server-and-test serve http://localhost:8080/ test-pa11y",
    "cypress-ci": "start-server-and-test serve http://localhost:8080/ cypress:run"
  },
  "dependencies": {
    "@carbon/icons-vue": "^10.22.0",
    "@carbon/vue": "^2.35.0",
    "bcryptjs": "^2.4.3",
    "core-js": "^2.6.12",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.1",
    "uws": "^10.148.2",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vue-socket.io": "^3.0.10",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "axios": "^0.19.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.6.0",
    "bootstrap": "^4.5.3",
    "cypress": "^6.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^6.2.2",
    "firebase": "^7.24.0",
    "jest-fetch-mock": "^2.1.2",
    "jquery": "^3.5.1",
    "node-fetch": "^2.6.1",
    "pa11y-ci": "^2.4.0",
    "popper.js": "^1.16.1",
    "sass": "^1.29.0",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.11.6",
    "vue-template-compiler": "^2.6.12",
    "vuefire": "^2.2.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "automock": false,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@carbon)/)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "setupFiles": [
      "./setupJest.js"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
